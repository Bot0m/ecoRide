{% extends 'base.html.twig' %}

{% block title %}Créer un employé{% endblock %}

{% block body %}
    <h1>Créer un nouvel employé</h1>

    {{ form_start(form) }}
        {{ form_row(form.pseudo) }}
        {{ form_row(form.email) }}
        {{ form_row(form.plainPassword) }}
        <button class="btn btn-primary">Créer</button>
    {{ form_end(form) }}
{% endblock %}